{
  "name": "riipandi/altstack",
  "type": "project",
  "description": "Alpine.js, Laravel, and Tailwind. An alternative full-stack development solution.",
  "keywords": [
    "framework",
    "alpinejs",
    "laravel",
    "tailwindcss"
  ],
  "homepage": "https://github.com/riipandi/altstack",
  "authors": [
    {
      "name": "Aris Ripandi",
      "email": "riipandi@gmail.com",
      "homepage": "https://github.com/riipandi"
    }
  ],
  "license": "Apache-2.0",
  "require": {
    "php": "^7.2.5",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-gettext": "*",
    "ext-gmp": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "blade-ui-kit/blade-heroicons": "^0.2.1",
    "blade-ui-kit/blade-icons": "^0.4.4",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": "^7.24",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.1",
    "league/flysystem-aws-s3-v3": "^1.0",
    "riipandi/laravel-optikey": "^1.2",
    "theiconic/name-parser": "^1.2",
    "watson/active": "^5.0"
  },
  "require-dev": {
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^9.3",
    "stefanbauer/tailwindcss-breakpoint-detector": "^1.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
